{
  "name": "multibundle",
  "version": "1.2.0",
  "description": "Runs requirejs bundling on multiple bundles in parallel",
  "main": "index.js",
  "scripts": {
    "test": "node test.js",
    "lint": "eslint -c .eslintrc {*,test/*}.js",
    "docs": "jsdoc2md --src index.js --private --name-format code --param-list-format list --sort-by scope access category kind order --group-by scope category --separators true > README.md"
  },
  "pre-commit": [
    "lint",
    "test",
    "docs"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:alexindigo/multibundle.git"
  },
  "keywords": [
    "amd",
    "requirejs",
    "bundle",
    "bundles",
    "multiple",
    "multi-bundle",
    "parallel"
  ],
  "author": "Alex Indigo <iam@alexindigo.com>",
  "license": "MIT",
  "dependencies": {
    "async": "^1.4.2",
    "glob": "^5.0.14",
    "lodash": "^3.10.1",
    "requirejs": "^2.1.20"
  },
  "devDependencies": {
    "backbone": "^1.2.3",
    "eslint": "^1.6.0",
    "jsdoc-to-markdown": "^1.2.0",
    "pre-commit": "^1.1.1",
    "rendr": "^1.1.1"
  }
}
