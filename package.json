{
  "name": "multibundle",
  "version": "2.0.0",
  "description": "Runs requirejs bundling on multiple bundles in parallel",
  "main": "index.js",
  "scripts": {
    "pretest": "rimraf ./test/tmp .nyc_output coverage && mkdirp test/tmp",
    "test": "nyc node test.js",
    "posttest": "nyc report --reporter=lcov",
    "lint": "eslint -c .eslintrc {*,test/*}.js"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:alexindigo/multibundle.git"
  },
  "keywords": [
    "amd",
    "requirejs",
    "bundle",
    "bundles",
    "multiple",
    "multi-bundle",
    "parallel"
  ],
  "author": "Alex Indigo <iam@alexindigo.com>",
  "license": "MIT",
  "dependencies": {
    "async": "^1.5.2",
    "glob": "^6.0.4",
    "lodash": "^4.11.1",
    "requirejs": "^2.1.22"
  },
  "devDependencies": {
    "backbone": "1.2.3",
    "coveralls": "^2.11.6",
    "deeply": "^1.0.0",
    "eslint": "^1.10.3",
    "mkdirp": "^0.5.1",
    "nyc": "^5.5.0",
    "pre-commit": "^1.1.2",
    "rendr": "1.1.3",
    "rimraf": "^2.5.1"
  }
}
